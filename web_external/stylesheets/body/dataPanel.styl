#m-data-panel
  text-align center

  .dataset
    padding-left 0px
    text-align left
    border 0
    margin 2px
    padding 2px

    i
      color #444
    .icon-disabled
      color #f0f0f0
    .icon-enabled:hover
      color #66a
      cursor pointer
    .icon-info-circled
      float right
    .icon-right-circled
      float left
    .icon-trash
      float right
    .icon-eye
      float left
