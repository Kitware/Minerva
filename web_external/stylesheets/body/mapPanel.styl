#m-map-panel
  height 100% !important
  width 100% !important

.m-map-panel-map
  width 100%
  height 100%
  
  &:focus
    outline none

// Remove geojs map focus highlight
.m-map-panel-map.geojs-map.highlight-focus:after
  display none

.m-map-legend-container
  position: absolute
  z-index: 400
  bottom: 0
  left: 0
