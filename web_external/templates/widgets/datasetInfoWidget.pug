.modal-dialog
  .modal-content
    form.modal-form(role="form")
      .modal-header
        button.close(data-dismiss="modal", aria-hidden="true", type="button") &times;
        h4.modal-title
        | Dataset Info
      .modal-body
        - var meta = normalizeMetaInfo()
        .m-dataset-info-table.clearfix
          .m-table-wrapper
            .m-key-column
              each key, index in Object.keys(meta)
                div(class=index%2===0?'odd':'even')= key
            .m-value-column
              each value, index in Object.values(meta)
                div(class=index%2===0?'odd':'even')= value
